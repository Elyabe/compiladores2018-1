// Teste com funcao max(x,y)

numero a, b, maior, menor, qtd_num, num;

exibeln max(2,-20);

a, b <== 20, log(45);

lecompleto "Digite a quantidade de numeros que deseja testar: ", qtd_num;

para ( maior, menor <== -1*(2**10), 2**10 ; qtd_num >= 0; qtd_num -= 1 )
	lecompleto "Digite um numero:", num;
	maior <- max( maior, num );
fim-para

exibeln "Maior: ", maior;
